@article{Masselon_2008_PRL,
   author = {Masselon, Chloe and Salmon, Jean-Baptiste and Colin,
                  Annie},
   title = {Nonlocal effects in flows of wormlike micellar solutions},
   journal = PRL,
   year = {2008},
   volume = {100},
   number = {3},
   month = jan,
   abstract = {The flow curve of wormlike micelles usually exhibits a
                  stress plateau sigma(star) separating high and low
                  viscosity branches, leading to shear-banded
                  flows. We study the flow of semidilute wormlike
                  micellar systems in a confined geometry: a straight
                  microchannel. We characterize their local rheology
                  thanks to particle image velocimetry.  We show that
                  flow curves cannot be described by a simple
                  constitutive equation linking the local shear stress
                  to the local shear rate. We demonstrate the
                  existence of nonlocal effects in the flow of
                  wormlike micellar systems and make use of a
                  theoretical framework allowing the measurement of
                  correlation lengths.},
   publisher = {{AMER. PHYSICAL SOC.}},
   doi = {{10.1103/PhysRevLett.100.038301}},
   pages = {038301},
   ISSN = {0031-9007},
   keywords-Plus = {{NONEQUILIBRIUM PHASE-TRANSITION; PARTICLE IMAGE
                  VELOCIMETRY; COMPLEX FLUIDS; CAPILLARY-FLOW;
                  SHEAR-FLOW; INSTABILITY; RHEOLOGY; BEHAVIOR;
                  STRESS}},
   subject-Category = {Physics, Multidisciplinary},
   author-Email = {{chloe.masselon-exterieur@eu.rhodia.com
                  annie.colin-exterieur@eu.rhodia.com}},
   journal-ISO = {{Phys. Rev. Lett.}},
   doc-Delivery-Number = {{255ZU}},
   Unique-ID = {{ISI:000252698100100}}
}

@article{Massiera_2002_EPL,
  Author = {Massiera, G. and Ramos, L. and Ligoure, C.},
  Title = {Role of the size distribution in the elasticity of
                  entangled living polymer solutions},
  Journal = EPL,
  Volume = {57},
  Number = {1},
  Pages = {127-133},
  Abstract = {The linear rheology of wormlike micelles decorated with
                  amphiphilic copolymer is reported. They are model
                  polymer solutions to address the role of
                  polydispersity in the scaling of the elastic plateau
                  modulus G(o) with concentration phi. Allowing only
                  micelles of coil size larger than the mesh size of
                  the network to contribute to its elasticity, we
                  account for the unusually fast increase of G(o) at
                  low phi. Our model enables a determination of the
                  micellar scission energy, whose decrease with
                  copolymer rate is independently confirmed by dynamic
                  light scattering.},
   Year = {2002}
}

