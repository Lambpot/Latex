@article{Lopez_2006_SM,
  Author = {Lopez-Gonzalez, M. R. and Holmes, W. M. and Callaghan,
                  P. T.},
  Title = {Rheo-NMR phenomena of wormlike micelles},
  Journal = SM,
  Volume = {2},
  Number = {10},
  Pages = {855-869},
  Abstract = {Using a combination of rheology and nuclear magnetic
                  resonance (NMR) spectroscopy/velocimetry we
                  demonstrate the existence of shear banding
                  fluctuations under Couette flow of the micellar
                  system 10\% w/v cetylpyridinium chloride and sodium
                  salicylate (CPyCl-NaSal) molar ratio 2 : 1 in 0.5 M
                  NaCl in either (H2O)-H-1 or (H2O)-H-2, using both
                  time-averaged and real-time measurements. These
                  shear banding fluctuations are consistent not only
                  with the shear stress fluctuations observed in
                  rheological measurements but also with fluctuations
                  in the change of the constrained fraction of the
                  amphiphile chain (Delta f(c)) observed in H-1-NMR
                  spectroscopy experiments. Using H-2-NMR spectroscopy
                  on a deuterated probe molecule (n-decane) located in
                  the wormlike micellar interior, direct measurement
                  of the shear-induced nematic phase transition is
                  reported.},
   Year = {2006}
}

