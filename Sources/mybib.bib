@article{Salmon_2003_PRE_03,
   author = {Salmon, Jean-Baptiste and Manneville, {S\'ebastien} and
                  Colin, Annie},
   title = {Shear banding in a lyotropic lamellar
                  phase. I. Time-averaged velocity profiles},
   journal = PRE,
   year = {2003},
   volume = {68},
   number = {{5, Part 1}},
   month = nov,
   abstract = {Using velocity profile measurements based on dynamic
                  light scattering and coupled to structural and
                  rheological measurements in a Couette cell, we
                  present evidences for a shear banding scenario in
                  the shear flow of the onion texture of a lyotropic
                  lamellar phase. Time-averaged measurements clearly
                  show the presence of structural shear banding in the
                  vicinity of a shear-induced transition, associated
                  with the nucleation and growth of a highly sheared
                  band in the flow. Our experiments also reveal the
                  presence of slip at the walls of the Couette
                  cell. Using a simple mechanical approach, we
                  demonstrate that our data confirm the classical
                  assumption of the shear banding picture, in which
                  the interface between bands lies at a given stress
                  sigma({*}).  We also outline the presence of large
                  temporal fluctuations of the flow field, which are
                  the subject of the second part of this paper
                  {[}Salmon , Phys. Rev. E 68, 051504 (2003)].},
   publisher = {{AMERICAN PHYSICAL SOC}}, 
   doi = {10.1103/PhysRevE.68.051503},
   pages = {051503}, 
   ISSN = {1063-651X},
   keywords-Plus = {{ISOTROPIC-NEMATIC TRANSITION; HOMODYNE
                  LIGHT-SCATTERING; WORMLIKE MICELLES; BANDED FLOW;
                  FLUID MEMBRANES; COMPLEX FLUIDS; BEHAVIOR; RHEOLOGY;
                  EMULSIONS; EQUILIBRIUM}}, 
   subject-Category = {{Physics, Fluids \& Plasmas; Physics,
                  Mathematical}}, 
   journal-ISO = {{Phys. Rev. E}},
   doc-Delivery-Number = {{752TN}}, 
   unique-ID = {{ISI:000187183400025}}
}

@article{Salmon_2003_PRE_04,
   author = {Salmon, Jean-Baptiste and Manneville, {S\'ebastien} and 
   Colin, Annie},
   title = {Shear banding in a lyotropic lamellar phase. II. Temporal 
   fluctuations},
   journal = PRE,
   year = {2003},
   volume = {68},
   number = {{5, Part 1}},
   month = nov,
   abstract = {We analyze the temporal fluctuations of the flow field
                  associated with a shear-induced transition in a
                  lyotropic lamellar phase: the layering transition of
                  the onion texture. In the first part of this work
                  {[}Salmon , Phys. Rev. E 68, 051503 (2003)], we have
                  evidenced banded flows at the onset of this
                  shear-induced transition which are well accounted
                  for by the classical picture of shear banding. In
                  the present paper, we focus on the temporal
                  fluctuations of the flow field recorded in the
                  coexistence domain. These striking dynamics are very
                  slow (100-1000 s) and cannot be due to external
                  mechanical noise. Using velocimetry coupled to
                  structural measurements, we show that these
                  fluctuations are due to a motion of the interface
                  separating the two differently sheared bands. Such a
                  motion seems to be governed by the fluctuations of
                  sigma({*}), the local stress at the interface
                  between the two bands. Our results thus provide more
                  evidence for the relevance of the classical
                  mechanical approach of shear banding even if the
                  mechanism leading to the fluctuations of sigma({*})
                  remains unclear.},
   publisher = {{AMERICAN PHYSICAL SOC}},
   doi = {10.1103/PhysRevE.68.051504},
   pages = {051504},
   ISSN = {{1063-651X}},
   keywords-Plus = {{WORMLIKE MICELLES; NEMATIC TRANSITION; COMPLEX
                  FLUIDS; FLOW; BEHAVIOR; SUSPENSIONS; RHEOLOGY;
                  STRESS}},
   subject-Category = {{Physics, Fluids \& Plasmas; Physics, Mathematical}},
   Journal-ISO = {{Phys. Rev. E}},
   Doc-Delivery-Number = {{752TN}},
   Unique-ID = {{ISI:000187183400026}}
}

